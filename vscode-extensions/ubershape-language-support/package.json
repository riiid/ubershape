{
  "name": "ubershape-language-support",
  "publisher": "Riiid",
  "repository": {
    "url": "git@github.com:riiid/ubershape.git",
    "type": "git"
  },
  "displayName": "Ubershape Language Support",
  "description": "Language Support for Ubershape.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "bin": {
    "install": "node ./bin/install-extension.js"
  },
  "scripts": {
    "prepublishOnly": "yarn bundle",
    "bundle": "mkdir -p dist && vsce package -o dist/ubershape.vsix"
  },
  "contributes": {
    "languages": [
      {
        "id": "ubershape",
        "aliases": [
          "Ubershape",
          "ubershape"
        ],
        "extensions": [
          "ubershape"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ubershape",
        "scopeName": "source.ubershape",
        "path": "./syntaxes/ubershape.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^1.81.1"
  }
}
